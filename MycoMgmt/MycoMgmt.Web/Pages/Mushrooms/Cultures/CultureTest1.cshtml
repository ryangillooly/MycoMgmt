@page "/culture/test"
@using Microsoft.AspNetCore.Antiforgery
@model IndexModel
@inject IAntiforgery antiforgery
@{
    var token = antiforgery.GetAndStoreTokens(HttpContext).RequestToken;
}
<h1>Index</h1>

<form method="post">
    <div class="form-group">
        <label for="input1">Name</label>
        <input type="text" class="form-control" id="name" name="name">
    </div>
    <div class="form-group">
        <label for="input2">Type</label>
        <input type="text" class="form-control" id="type" name="type">
    </div>
    <div class="form-group">
        <label for="input3">Strain</label>
        <input type="text" class="form-control" id="strain" name="strain">
    </div>
    <div class="form-group">
        <label for="input2">Created On</label>
        <input type="text" class="form-control" id="createdOn" name="createdOn">
    </div>
    <div class="form-group">
        <label for="input3">Created By</label>
        <input type="text" class="form-control" id="createdBy" name="createdBy">
    </div>
    
    <button type="submit" class="btn btn-primary">Submit</button>
</form>

